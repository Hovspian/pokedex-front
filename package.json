{
  "name": "pokedex-front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-infinite-scroller": "^1.2.1",
    "react-scripts": "1.1.5",
    "reactstrap": "^6.4.0",
    "merge": ">=1.2.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.0",
    "eslint-config-google": "^0.10.0",
    "eslint-plugin-react": "^7.11.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:debug": "react-scripts --inspect-brk test --runInBand",
    "eject": "react-scripts eject",
    "test:nowatch": "cross-env CI=true react-scripts test --env=jsdom --outputFile=results.json --json",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "lint": "eslint src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/js/**/*.js"
    ]
  }
}
